{
    "extends": "dtslint/dtslint.json",
    "rules": {
        "semicolon": false,
        "no-consecutive-blank-lines": false,
        "no-trailing-whitespace": false,
        "no-single-declare-module": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-null-keyword": false
    },
    "jsRules": true
}
